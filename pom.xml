<?xml version="1.0" encoding="UTF-8"?>
<project xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
				 xmlns="http://maven.apache.org/POM/4.0.0"
				 xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/xsd/maven-4.0.0.xsd">
	<modelVersion>4.0.0</modelVersion>
	<packaging>pom</packaging>

	<parent>
		<groupId>org.springframework.boot</groupId>
		<artifactId>spring-boot-starter-parent</artifactId>
		<version>2.7.8</version>
		<relativePath/> <!-- lookup parent from repository -->
	</parent>

	<groupId>no.nav.soknad.innsending</groupId>
	<artifactId>innsending-api</artifactId>
	<version>1.0.0</version>
	<name>innsending-api</name>
	<description>Tjeneste for mottak av dokumenter og videresending til soknadsmottakstjenesten</description>

	<properties>
		<java.version>17</java.version>
		<kotlin.version>1.7.20</kotlin.version>

		<maven.compiler.source>${java.version}</maven.compiler.source>
		<maven.compiler.target>${java.version}</maven.compiler.target>
		<project.build.sourceEncoding>UTF-8</project.build.sourceEncoding>

		<!--Spring-->
		<spring-boot.version>2.7.8</spring-boot.version>
		<spring-web.version>5.3.23</spring-web.version>
		<spring-cloud.version>3.1.4</spring-cloud.version>

		<!--OpenAPI-->
		<springdoc-openapi.version>1.6.12</springdoc-openapi.version>
		<swagger-annotations.version>2.2.4</swagger-annotations.version>

		<!--Token-->
		<token-validation.version>2.1.7</token-validation.version>
		<mock-oauth2-server.version>0.5.7</mock-oauth2-server.version>

		<kotlin-serialization.version>1.4.1</kotlin-serialization.version>

		<!--Database-->
		<otj-pg-embedded.version>1.0.1</otj-pg-embedded.version>
		<flyway.version>9.6.0</flyway.version>

		<!--Other-->
		<okhttp3.version>4.10.0</okhttp3.version>
		<io-netty.version>4.1.86.Final</io-netty.version>
		<okio.version>3.2.0</okio.version>
		<kafka.version>3.4.0</kafka.version>
		<javax-servlet-api.version>4.0.1</javax-servlet-api.version>
		<javax-validation-api.version>2.0.1.Final</javax-validation-api.version>
		<jakarta-annotation.version>2.1.1</jakarta-annotation.version>
		<jackson-databind-nullable.version>0.2.4</jackson-databind-nullable.version>
		<jackson.version>2.13.4</jackson.version>
		<coroutines.version>1.6.4</coroutines.version>
		<coroutines.version>1.6.4</coroutines.version>

		<prometheus-client.version>0.16.0</prometheus-client.version>
		<logstash.version>7.2</logstash.version>
		<tika-core.version>2.5.0</tika-core.version>
		<pdfbox.version>2.0.27</pdfbox.version>
		<imgscalr-lib.version>4.2</imgscalr-lib.version>
		<mockk.version>1.13.2</mockk.version>
		<resilience4j.version>1.7.1</resilience4j.version>
		<graphql-kotlin.version>6.2.5</graphql-kotlin.version>
		<gson.version>2.10</gson.version>

		<!--Plugins-->
		<versions-maven-plugin>2.15.0</versions-maven-plugin>
		<maven-surefire-plugin.version>3.0.0-M7</maven-surefire-plugin.version>
		<openapi-generator-maven-plugin.version>6.2.0</openapi-generator-maven-plugin.version>

	</properties>

	<modules>
		<module>api</module>
		<module>soknadsfillager-api</module>
		<module>soknadsmottaker-api</module>
		<module>innsender</module>
	</modules>

	<repositories>
		<repository>
			<id>central</id>
			<url>https://repo1.maven.org/maven2</url>
		</repository>
	</repositories>

</project>
